


{% extends 'gdPublic/partial/GdPublicHeaderFooter.html.twig' %}

{% block metatitle %}
    blog : les articles
{% endblock %}

{% block content %}

<body>
    <main>
        <h1> Article : </h1>
        <div class="document">
                    <div class="articles">
                        <article class="article">
                            <h2 class="titre">  {{ article.title }}</h2>
                            <p> {{ article.description }}</p>
                            <p> {{ article.content }}</p>
                            <div class="image">
                                <img src="{{ asset(article.image) }}" alt="{{ article.title }}">
                            </div>
                        </article>
                    </div>
        </div>
    </main>
</body>

{% endblock %}


